    "import math\n",
    "import numpy as np\n",
    "\n",
    "Q=np.array([1,2,3,4])\n",
    "K=np.array([2,3,4,5])\n",
    "V=np.array([0,1,2,3])\n",
    "scale=3\n",
    "\n",
    "diff=abs(np.sum(Q-K))\n",
    "print(diff)\n",
    "S=math.e**(-scale*diff)\n",
    "\n",
    "print(\"S: \",S)\n",
    "\n",
    "Z=S*V\n",
    "print(\"Z: \",Z)\n"
   ]
